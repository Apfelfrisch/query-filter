{
    "name": "apfelfrisch/query-filter",
    "description": "A Framework agnostic query filter that sorts and filters queries using URL parameters",
    "license": "MIT",
    "require": {
        "php": "~8.1.0 || ~8.2.0 || ~8.3.0 || ~8.4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.5.12",
        "symfony/var-dumper": "^5.4.36",
        "phpstan/phpstan": "^1.10.60",
        "phpstan/phpstan-phpunit": "^1.3.16",
        "illuminate/database": "^9.52.16",
        "doctrine/dbal": "^3.8.3",
        "infection/infection": "^0.26.21",
        "friendsofphp/php-cs-fixer": "^3.51"
    },
    "autoload": {
        "psr-4": {
            "Apfelfrisch\\QueryFilter\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Apfelfrisch\\QueryFilter\\Tests\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "infection/extension-installer": true
        }
    }
}
